{
    "kind": "collectionType",
    "collectionName": "services",
    "info": {
        "singularName": "service",
        "pluralName": "services",
        "displayName": "Service",
        "description": ""
    },
    "options": {
        "draftAndPublish": true
    },
    "attributes": {
        "title": {
            "type": "string",
            "unique": true,
            "required": true
        },
        "serviceDescription": {
            "type": "blocks",
            "required": true
        },
        "displayOrder": {
            "type": "integer",
            "default": 0,
            "min": 0
        },
        "category": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::service-category.service-category",
            "inversedBy": "services"
        },
        "slug": {
            "type": "uid",
            "targetField": "title",
            "required": true
        },
        "icon": {
            "type": "media",
            "multiple": false,
            "required": true,
            "allowedTypes": [
                "images",
                "files"
            ]
        },
        "projects": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::project.project",
            "mappedBy": "service"
        }
    }
}
