{
    "kind": "collectionType",
    "collectionName": "service_categories",
    "info": {
        "singularName": "service-category",
        "pluralName": "service-categories",
        "displayName": "ServiceCategory",
        "description": ""
    },
    "options": {
        "draftAndPublish": true
    },
    "attributes": {
        "name": {
            "type": "string",
            "required": true,
            "unique": true
        },
        "slug": {
            "type": "uid",
            "targetField": "name",
            "required": true
        },
        "categoryDescription": {
            "type": "blocks",
            "required": true
        },
        "displayOrder": {
            "type": "integer",
            "default": 0,
            "min": 0
        },
        "services": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::service.service",
            "mappedBy": "category"
        }
    }
}
